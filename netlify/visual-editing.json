{
  "version": 1,
  "siteId": "numeriqueresponsable.netlify.app",
  "contentTypes": {
    "pages": {
      "path": "content",
      "fields": {
        "title": {
          "type": "string",
          "required": true
        },
        "body": {
          "type": "markdown"
        }
      }
    },
    "articles": {
      "path": "content/articles",
      "fields": {
        "title": {
          "type": "string",
          "required": true
        },
        "body": {
          "type": "markdown"
        }
      }
    }
  },
  "preview": {
    "enabled": true,
    "url": "https://numeriqueresponsable.netlify.app"
  },
  "permissions": {
    "create": ["admin", "editor"],
    "read": ["admin", "editor"],
    "update": ["admin", "editor"],
    "delete": ["admin"],
    "publish": ["admin", "editor"]
  }
} 